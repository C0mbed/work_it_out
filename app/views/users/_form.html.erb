<%= f.text_field :first_name, class: 'fadeIn first', placeholder: 'first name' %>
<%= f.text_field :last_name, class: 'fadeIn second', placeholder: 'last name' %>

<%= f.text_field :email, class: 'fadeIn third', placeholder: 'email' %>
<%= f.password_field :password, class: 'fadeIn fourth', placeholder: 'password' %>
<%= f.password_field :password_confirmation, class: 'fadeIn fifth', placeholder: 'confirm password' %>

<%= f.submit "submit" %>
