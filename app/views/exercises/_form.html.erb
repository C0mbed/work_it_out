<% if params[:workout_id] %>
    <%= f.hidden_field :workout_id, :value => params[:workout_id] %>
    <% byebug %>
<% end %>

<%= f.text_field :name, class: 'fadeIn first', placeholder: 'exercise name' %>

<%= f.text_field :exercise_type, class: 'fadeIn second', placeholder: 'exercise type' %>

<%= f.text_field :equipment, class: 'fadeIn third', placeholder: 'equipment' %>

<%= f.text_field :description, class: 'fadeIn fourth', placeholder: 'description' %>

<%= f.submit "submit" %>